<apex:page standardController="Contact" extensions="SECCustomNewContactExt">

     <apex:form >
         <apex:pageBlock title="Contact Edit" mode="maindetail">
             <apex:pageBlockButtons >
                 <apex:commandButton action="{!save}" value="Save"/>
             </apex:pageBlockButtons>
             <apex:pageBlockSection title="Contact Information" columns="2">
                 <apex:outputField value="{!con.OwnerId}"/>
                 <apex:inputField value="{!con.Status__c}"/>
                 <apex:inputField value="{!con.FirstName}"/>
                 <apex:inputField value="{!con.Phone}"/>
                 <apex:inputField value="{!con.LastName}"/>
                 <apex:inputField value="{!con.Fax}"/>
                 <apex:inputField value="{!con.Preferred_Name__c}"/>
                 <apex:inputField value="{!con.OtherPhone}"/>
                 <apex:inputField value="{!con.AccountId}" required="true"/>
                 <apex:inputField value="{!con.MobilePhone}"/>
                 <apex:inputField value="{!con.Title}"/>
                 <apex:inputField value="{!con.Email}"/>
                 <apex:inputField value="{!con.Suffixes__c}"/>
                 <apex:inputField value="{!con.Additional_email_address__c}"/>
                 <apex:inputField value="{!con.Other_Suffixes__c}"/>
                 <apex:inputField value="{!con.Preferred_Communication_Method__c}"/>
                 <apex:inputField value="{!con.Lilly_Person_ID__c}"/>
                 <apex:inputField value="{!con.Preferred_Meeting_Day__c}"/>
                 <apex:outputText value=""/>
                 <apex:inputField value="{!con.Impact_Investigator_Number__c}"/>
             </apex:pageBlockSection>
             <apex:pageBlockSection title="Address Information" columns="2">
                 <apex:inputField value="{!con.MailingStreet}" />
			     <apex:inputField value="{!con.OtherStreet}" />
				 <apex:inputField value="{!con.MailingCity}" />
				 <apex:inputField value="{!con.OtherCity}" />
				 <apex:inputField value="{!con.MailingState}" />
				 <apex:inputField value="{!con.OtherState}" />
				 <apex:inputField value="{!con.MailingPostalCode}" />
				 <apex:inputField value="{!con.OtherPostalCode}" />
				 <apex:inputField value="{!con.MailingCountry}" />
				 <apex:inputField value="{!con.OtherCountry}" />
             </apex:pageBlockSection>
             <apex:pageBlockSection title="Additional Information" columns="2">
                 <apex:inputField value="{!con.Department}"/>
                 <apex:inputField value="{!con.LeadSource}"/>
                 <apex:inputField value="{!con.Therapeutic_Area__c}" required="true"/>
                 <apex:inputField value="{!con.Language_s__c}"/>
                 <apex:inputField value="{!con.Role__c}" required="true"/>
                 <apex:inputField value="{!con.AssistantName}"/>
                 <apex:inputField value="{!con.Specialty__c}" required="true"/>
                 <apex:inputField value="{!con.AssistantPhone}"/>
                 <apex:inputField value="{!con.Study_Type_Interest__c}"/>
                 <apex:inputField value="{!con.Asst_Email__c}"/>
                 <apex:outputText value=""/>
                 <apex:inputField value="{!con.Study_Type_Interest__c}"/>
             </apex:pageBlockSection>
         </apex:pageBlock>
     </apex:form>
</apex:page>